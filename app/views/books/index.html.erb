!index!
<%#= render 'users/user', user: @user %>
<%= render partial: 'users/user', locals: { user: @user } %>
<%= render partial: 'books', locals: { book: @book } %>


<% @books.each do |book| %>
  <div>
    <%= image_tag book.get_image %>
    <%= link_to book.title, book_path(current_user) %>
    <%= book.body %>
  </div>
<% end %>

<%= link_to books_path(@book.id) do %>
  <% end %>